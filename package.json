        "label": "Electron Build Tools: Build Output",
        "fileLocation": [
          "relative",
          "${command:electron-build-tools.show.out.path}"
        ],
        "applyTo": "allDocuments",
        "pattern": {
          "regexp": "^(.*):(\\d+):(\\d+):\\s+(?:fatal )?(warning|error):\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      }
    ],

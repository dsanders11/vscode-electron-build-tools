{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2018",
    "outDir": "out",
    "lib": [
      "ES2020"
    ],
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react",
    "jsxFactory": "vscpp",
    "jsxFragmentFactory": "vscppf"
  },
  "include": ["src"],
  "exclude": ["electron", "scripts"]
}
